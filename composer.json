{
    "name": "ycs77/laravel-form-field-type",
    "type": "library",
    "description": "Fast set the form fields of the Laravel form builder.",
    "keywords": [
        "ycs77",
        "laravel-form-field-type"
    ],
    "homepage": "https://github.com/ycs77/laravel-form-field-type",
    "license": "MIT",
    "authors": [
        {
            "name": "Oscar Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "kris/laravel-form-builder": "^1.16",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Ycs77\\LaravelFormFieldType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Forms\\Fields\\": "src/Fields/",
            "Ycs77\\LaravelFormFieldType\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycs77\\LaravelFormFieldType\\FieldTypeServiceProvider"
            ],
            "aliases": {
                "FieldType": "Ycs77\\LaravelFormFieldType\\Facades\\FieldType"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
